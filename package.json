{
  "name": "druid-query-toolkit",
  "version": "0.14.24",
  "description": "A collection of utilities for working with Druid queries",
  "main": "dist/druid-query-toolkit.js",
  "module": "dist/druid-query-toolkit.mjs",
  "types": "types/index.d.ts",
  "homepage": "https://github.com/implydata/druid-query-toolkit",
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "14.16.1",
    "npm": "6.14.12"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/?(*.)+(spec).ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>jest-env.ts"
    ]
  },
  "prettier": "@awesome-code-style/prettier-config",
  "tsup": {
    "entry": {
      "druid-query-toolkit": "src/index.ts"
    },
    "format": [
      "esm",
      "cjs",
      "iife"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "build:types": "rm -rf types && tsc --emitDeclarationOnly",
    "build": "tsup --global-name druid",
    "prepublishOnly": "npm run build:types && npm run build",
    "pretest": "npm run compile",
    "test": "npm run eslint && npm run prettify-check && jest",
    "coverage": "jest --coverage",
    "compile": "rm -rf build/ && node script/compile-peg.js && tsc && npm run cp-parser",
    "cp-parser": "cp -r src/sql/parser build/sql/ && rm build/sql/parser/*.spec.ts",
    "eslint": "eslint '{src,e2e-tests}/**/*.ts?(x)'",
    "eslint-fix": "npm run eslint -- --fix",
    "eslint-changed-only": "git diff --diff-filter=ACMR --cached --name-only | grep -E \\.tsx\\?$ | xargs ./node_modules/.bin/eslint",
    "eslint-fix-changed-only": "npm run eslint-changed-only -- --fix",
    "prettify": "prettier --write '{src,e2e-tests}/**/*.{ts,tsx,scss}' './*.js'",
    "prettify-check": "prettier --check '{src,e2e-tests}/**/*.{ts,tsx,scss}' './*.js'",
    "prepare": "husky install"
  },
  "keywords": [
    "sql",
    "sql-parser",
    "parser",
    "node",
    "node-parser",
    "node-sql-parser",
    "ast",
    "sql-ast"
  ],
  "author": "Imply Data",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/implydata/druid-query-toolkit"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@awesome-code-style/eslint-config": "^4.0.0",
    "@awesome-code-style/prettier-config": "^4.0.0",
    "@jest/core": "27.5.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "axios": "^0.26.1",
    "core-js": "^3.21.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "pegjs": "^0.10.0",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "tsup": "^6.1.2",
    "typescript": "^4.6.3"
  }
}
