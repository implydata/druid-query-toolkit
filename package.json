{
  "name": "druid-query-toolkit",
  "version": "0.3.4",
  "description": "A collection of utilities for working with Druid queries\n",
  "main": "build/index.ts",
  "homepage": "https://github.com/implydata/druid-sql-parser",
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/?(*.)+(spec).ts"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf"
  },
  "scripts": {
    "test": "jest",
    "playground": "node script/compile-peg.js && jest src/playground.spec.ts",
    "compile": "node script/compile-peg.js && tsc"
  },
  "keywords": [
    "sql",
    "sql-parser",
    "parser",
    "node",
    "node-parser",
    "node-sql-parser",
    "ast",
    "sql-ast"
  ],
  "author": "Imply Data",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "awesome-code-style": "^1.4.3",
    "jest": "^24.8.0",
    "pegjs": "^0.10.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  }
}
